  "statusLine": {
    "type": "command",
    "command": "input=$(cat); project_dir=$(echo \"$input\" | jq -r '.workspace.project_dir // .workspace.current_dir'); current_dir=$(echo \"$input\" | jq -r '.workspace.current_dir'); model=$(echo \"$input\" | jq -r '.model.display_name'); total_in=$(echo \"$input\" | jq -r '.context_window.total_input_tokens // 0'); total_out=$(echo \"$input\" | jq -r '.context_window.total_output_tokens // 0'); ctx_size=$(echo \"$input\" | jq -r '.context_window.context_window_size // 200000'); user_host=\"$(whoami)@$(hostname -s)\"; if [ \"$current_dir\" = \"$project_dir\" ]; then rel_dir=\".\"; else rel_dir=$(echo \"$current_dir\" | sed \"s|^$project_dir/||\" | sed 's|.*/||'); fi; git_info=\"\"; if git rev-parse --git-dir > /dev/null 2>&1; then branch=$(git symbolic-ref --short HEAD 2>/dev/null || git rev-parse --short HEAD 2>/dev/null); if [ -n \"$branch\" ]; then status_output=$(git status --porcelain 2>/dev/null); staged=$(echo \"$status_output\" | grep -c '^[MADRC]' || echo 0); modified=$(echo \"$status_output\" | grep -c '^ [MD]' || echo 0); untracked=$(echo \"$status_output\" | grep -c '^??' || echo 0); indicators=\"\"; [ \"$staged\" -gt 0 ] && indicators=\"${indicators}+${staged}\"; [ \"$modified\" -gt 0 ] && indicators=\"${indicators}~${modified}\"; [ \"$untracked\" -gt 0 ] && indicators=\"${indicators}?${untracked}\"; if [ -n \"$indicators\" ]; then git_info=\" (${branch}:${indicators})\"; else git_info=\" (${branch})\"; fi; fi; fi; lang_info=\"\"; if [ -n \"$VIRTUAL_ENV\" ] || [ -f \"$current_dir/pyproject.toml\" ] || [ -f \"$current_dir/requirements.txt\" ]; then py_ver=$(python3 --version 2>/dev/null | awk '{print $2}' | cut -d. -f1-2); [ -n \"$py_ver\" ] && lang_info=\"${lang_info} py:${py_ver}\"; fi; if [ -f \"$current_dir/package.json\" ]; then node_ver=$(node --version 2>/dev/null | sed 's/^v//' | cut -d. -f1-2); [ -n \"$node_ver\" ] && lang_info=\"${lang_info} node:${node_ver}\"; fi; if [ -f \"$current_dir/go.mod\" ]; then go_ver=$(go version 2>/dev/null | awk '{print $3}' | sed 's/go//' | cut -d. -f1-2); [ -n \"$go_ver\" ] && lang_info=\"${lang_info} go:${go_ver}\"; fi; total_tokens=$((total_in + total_out)); ctx_pct=$((ctx_size > 0 ? total_tokens * 100 / ctx_size : 0)); printf \"%s:%s%s%s • %s • %d%%\" \"$user_host\" \"$rel_dir\" \"$git_info\" \"$lang_info\" \"$model\" \"$ctx_pct\""
  },
  "outputStyle": "odin:ODIN",
  "spinnerTipsEnabled": false,
  "alwaysThinkingEnabled": true
